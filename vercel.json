{
  "buildCommand": "cd frontend && NPM_CONFIG_PRODUCTION=false npm ci && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && NPM_CONFIG_PRODUCTION=false npm ci",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}